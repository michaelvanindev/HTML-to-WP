{
  "$schema": "https://schemas.wp.org/wp/6.5/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "palette": [
        { "slug": "mv-white", "color": "rgb(255,255,255)", "name": "MV White" },
        { "slug": "mv-gray-100", "color": "rgb(243 244 246)", "name": "MV Gray 100" },
        { "slug": "mv-gray-200", "color": "rgb(229 231 235)", "name": "MV Gray 200" },
        { "slug": "mv-gray-300", "color": "rgb(209 213 219)", "name": "MV Gray 300" },
        { "slug": "mv-gray-400", "color": "rgb(156 163 175)", "name": "MV Gray 400" },
        { "slug": "mv-gray-500", "color": "rgb(107 114 128)", "name": "MV Gray 500" },
        { "slug": "mv-gray-600", "color": "rgb(75 85 99)", "name": "MV Gray 600" },
        { "slug": "mv-gray-700", "color": "rgb(55 65 81)", "name": "MV Gray 700" },
        { "slug": "mv-gray-800", "color": "rgb(31 41 55)", "name": "MV Gray 800" },
        { "slug": "mv-gray-900", "color": "rgb(17 24 39)", "name": "MV Gray 900" },
        { "slug": "mv-primary", "color": "rgb(239 68 68)", "name": "MV Primary" },
        { "slug": "mv-accent", "color": "rgb(67 56 202)", "name": "MV Accent" },
        { "slug": "mv-success", "color": "rgb(52 211 153)", "name": "MV Success" },
        { "slug": "mv-info", "color": "rgb(96 165 250)", "name": "MV Info" },
        { "slug": "mv-warning", "color": "rgb(251 146 60)", "name": "MV Warning" },
        { "slug": "mv-danger", "color": "rgb(236 72 153)", "name": "MV Danger" }
      ],
      "background": true,
      "text": true,
      "link": true,
      "custom": true,
      "duotone": [
        { "slug": "mv-pink-sunset", "colors": ["#11235e", "#dc4379"], "name": "MV Pink Sunset" }
      ],
      "defaultGradients": false,
      "gradients": [
        {
          "slug": "mv-summer-dog",
          "name": "MV Summer Dog",
          "gradient": "linear-gradient(90deg, red, yellow)"
        }
      ]
    },
    "blocks": {
      "core/site-title": {
        "color": {
          "defaultPalette": true,
          "palette": [
            { "slug": "mv-primary", "color": "#ef4444", "name": "MV Primary 2" }
          ],
          "link": true,
          "custom": false,
          "gradients": [],
          "customGradient": false
        },
        "typography": {
          "fontFamilies": [
            { "fontFamily": "Paifico, sans-serif",
              "slug": "mv-pacifico",
              "name": "MV Pacifico"
            }
          ]
        }
      },
      "core/cover": {
        "color": {
          "duotone": [],
          "customDuotone": false
        }
      },
      "core/preformatted": {
        "typography": {
          "fontSizes": [],
          "customFontSize": false
        }
      }
    },
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "typography": {
      "fontFamilies": [
        { "fontFamily": "Rubik, sans-serif",
          "slug": "mv-rubik",
          "name": "MV Rubik"
        }
      ],
      "fontSizes": [
        { "slug": "small", "size": "0.75rem", "name": "Small" },
        { "slug": "medium", "size": "1.25rem", "name": "Medium" },
        { "slug": "large", "size": "2.25rem", "name": "Large" },
        { "slug": "x-large", "size": "2rem", "name": "X-Large" },
        { "slug": "gigantic", "size": "3.75rem", "name": "Gigantic" }
      ],
      "lineHeight": true,
      "dropCap": true,
      "fontWeight": true,
      "fontStyle": true,
      "textTransform": true,
      "letterSpacing": true,
      "textDecoration": true
    },
    "layout": {
      "contentSize": "800px",
      "wideSize": "1100px"
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "units": [
        "px", "rem", "vh", "vw", "%"
      ],
      "blockGap": true
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--mv-gray-900)",
      "background": "var(--wp--preset--color--mv-white)"
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--mv-primary)"
        }
      }
    },
    "blocks": {
      "core/button": {
        "color": {
          "gradient": "var(--wp--preset--gradient--mv-summer-dog)"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "2px",
          "radius": "10px",
          "style": "solid",
          "color": "var(--wp--preset--color--mv-primary)"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--mv-pacifico)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--mv-rubik)",
      "fontSize": "16px",
      "fontStyle": "normal",
      "fontWeight": "normal",
      "lineHeight": "inherit",
      "textDecoration": "none",
      "textTransform": "none:"
    },
    "spacing": {
      "margin": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "padding": {
        "top": "0px",
        "bottom": "0px",
        "left": "0px",
        "right": "0px"
      },
      "blockGap": "4rem",
      "test": "test"
    }
  }
}